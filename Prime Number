class Solution{
    static int isPrime(int N){
        // code here
        int count=0;
        if(N==1){
            return 0;
        }
        else{
        for(int i=2;i<=Math.sqrt(N);i++){
            if(N%i==0){
                return 0;
            }
        }
        return 1;
        }
    }
}
